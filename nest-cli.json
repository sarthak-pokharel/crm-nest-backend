{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/crm/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/crm/tsconfig.app.json"
  },
  "projects": {
    "crm": {
      "type": "application",
      "root": "apps/crm",
      "entryFile": "main",
      "sourceRoot": "apps/crm/src",
      "compilerOptions": {
        "tsConfigPath": "apps/crm/tsconfig.app.json"
      }
    },
    "email-service": {
      "type": "application",
      "root": "apps/email-service",
      "entryFile": "main",
      "sourceRoot": "apps/email-service/src",
      "compilerOptions": {
        "tsConfigPath": "apps/email-service/tsconfig.app.json"
      }
    },
    "libs": {
      "type": "library",
      "root": "libs/common",
      "entryFile": "index",
      "sourceRoot": "libs/common/src",
      "compilerOptions": {
        "tsConfigPath": "libs/common/tsconfig.lib.json"
      }
    }
  },
  "monorepo": true,
  "root": "apps/crm"
}